<%- include('partials/header') -%>
<h1>Compose</h1>
<form method="post" action="/compose">
    <div class="form-group">
        <label for="publishTitle">Title</label>
        <input class="form-control" type="text" name="publishTitle" id="publishTitle">
        <label for="publishPost">Post</label>
        <textarea class="form-control" rows="5" cols="30" name="publishPost" id="publishPost"></textarea>
    </div>
    <button class="btn btn-info"type="submit" name="button">Publish</button>
</form>
<%- include('partials/footer') -%>